replica_module = import_da('replica')


class Olympus(process):
    def setup(num_replica:int):
        output("olympus: setup finished num_replica:", num_replica)

    def run():
        # creating num_replica replicas
        output("olympus:run started")
        replicas = new(replica_module.Replica, [], num=num_replica)
        start(replicas)
        output("olympus: run finished")


def main():
    olympus = new(Olympus, [ 1 ])
    start(olympus)
